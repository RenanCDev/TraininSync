[MASTER]
load-plugins = pylint_django

[MESSAGES CONTROL]
disable =
    missing-docstring,
    invalid-name,
    too-few-public-methods,
    no-member,
    too-many-arguments,
    too-many-locals,
    too-many-branches,
    too-many-statements,
    too-many-instance-attributes,
    duplicate-code

[FORMAT]
max-line-length = 88

[DESIGN]
max-args = 10
max-locals = 25
